{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "semi": "off",
//    "linebreak-style": ["error", "windows"],
    "linebreak-style": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "comma-spacing": "off",
    "new-cap": "off",
    "keyword-spacing": "off",
    "operator-linebreak": "off",
    "no-trailing-spaces": "off"
  },
  "extends":["eslint:recommended", "google"]
}